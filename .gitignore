*.xcodeproj
*.xcworkspace
*.sln
*.suo
*.opensdf
*.sdf
*.VC.db
*.VC.opendb

# Precompiled Assets
ProjectDS/SourceArt/**/*.png
ProjectDS/SourceArt/**/*.tga

# Binary Files
ProjectDS/Binaries/*
ProjectDS/Plugins/*/Binaries/*

# Builds
ProjectDS/Build/*

# Whitelist PakBlacklist-<BuildConfiguration>.txt files
!ProjectDS/Build/*/
ProjectDS/Build/*/**
!ProjectDS/Build/*/PakBlacklist*.txt

# Don't ignore icon files in Build
!ProjectDS/Build/**/*.ico

# Built data for maps
ProjectDS/*_BuiltData.uasset

# Configuration files generated by the Editor
ProjectDS/Saved/*

# Compiled source files for the engine to use
ProjectDS/Intermediate/*
ProjectDS/Plugins/*/Intermediate/*

# Cache files for the editor to use
ProjectDS/DerivedDataCache/*
